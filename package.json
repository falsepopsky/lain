{
  "name": "lain",
  "private": true,
  "type": "module",
  "license": "Apache-2.0",
  "scripts": {
    "format": "prettier --cache -w .",
    "lint": "eslint . --cache --cache-location \"./node_modules/.cache/eslint/\"",
    "clean": "rm -rf node_modules .turbo",
    "clean:build": "turbo clean:build",
    "clean:workspace": "turbo clean:workspace",
    "build": "turbo build --filter=!eslint-config-lain",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "preinstall": "npx only-allow pnpm",
    "publish-packages": "pnpm run build && changeset publish",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@swc/core": "^1.3.99",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.10",
    "eslint": "^8.54.0",
    "eslint-config-lain": "workspace:^",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "turbo": "^1.10.16",
    "typescript": "^5.3.2",
    "vitepress": "1.0.0-rc.30"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "pnpm": {
    "overrides": {
      "esbuild": "^0.19.7"
    }
  },
  "packageManager": "pnpm@8.10.5"
}
