{
  "name": "@untidy/animeflv",
  "description": "web scraping tools for animeflv site",
  "version": "0.2.1",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./url": {
      "types": "./dist/url.d.ts",
      "import": "./dist/url.js"
    },
    "./cheerio": {
      "types": "./dist/cheerio.d.ts",
      "import": "./dist/cheerio.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "animeflv"
  ],
  "author": "falsepopsky",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "tsx ./src/playground.ts",
    "clean:build": "rm -rf dist",
    "clean:workspace": "rm -rf dist .turbo",
    "prebuild": "pnpm run clean:build",
    "build": "tsc --project tsconfig.build.json"
  },
  "devDependencies": {
    "@types/node": "^20.8.6",
    "cheerio": "1.0.0-rc.12",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "cheerio": "^1.0.0-rc.12"
  },
  "peerDependenciesMeta": {
    "cheerio": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/falsepopsky/lain.git",
    "directory": "packages/animeflv"
  },
  "bugs": {
    "url": "https://github.com/falsepopsky/lain/issues"
  },
  "homepage": "https://lain-packages.netlify.app/animeflv/"
}
